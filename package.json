  "scripts": {
    "test": "NODE_ENV=development mocha test/**/*.spec.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --https --cert=/Users/myowngrave/server.crt --key=/Users/myowngrave/server.key --port 8081 --progress",
    "dev:node": "redis-cli flushall;NODE_ENV=development nodemon --inspect --trace-warnings $NODE_DEBUG_OPTION --watch server-dist bin/www",
    "dev:node:only": "redis-cli flushall;NODE_ENV=development node --inspect bin/www",
    "dev:test": "NODE_ENV=development nodemon --trace-warnings $NODE_DEBUG_OPTION --watch server server/test.dev.js",
    "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "server:reload": "ssh root.myowngrave 'redis-cli flushall; pm2 restart vaithuhay --interpreter=`which node`'",
    "server:reload:debug": "ssh root.myowngrave 'redis-cli flushall; pm2 restart vaithuhay --interpreter=`which node` --node-args='--inspect=0.0.0.0:9229''",
    "server:watch": "cross-env NODE_ENV=development webpack --config webpack.backend.config.js --hide-modules --color --watch",
    "server:compile": "cross-env NODE_ENV=production webpack --config webpack.backend.config.js",
    "sync": "NODE_ENV=development APP_RUNTIME_ENV=cli node server-dist/test.dev.js spreadsheet",
    "cli-dev": "NODE_ENV=development APP_RUNTIME_ENV=cli node server-dist/test.dev.js"
  },
